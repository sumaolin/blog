<%- include header %>

<% posts.forEach(function(post, index){ %>

	<p><h2><a href="/u/<%= post.title %>"><%= post.title %></a></h2></p>
	<p class="info">
		作者:<a href="/u/<%= post.name %>"><%= post.name %></a>
		日期:<a href="/u/<%= post.time.day %>"><%= post.time.day %></a>
	</p>
	<p><%- post.post %></p>

<% }) %>

<%- include footer %>
